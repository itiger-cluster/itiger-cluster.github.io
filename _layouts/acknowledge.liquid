---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title">{{ page.title }}</h1>
    <p class="post-description">{{ page.description }}</p>
  </header>

  <div class="container">
		<div class='row'>
		<div class="col-md-9">
		<article>
    {{ content }}
    </article>
		</div>
		<div class="col-md-3">
		<img src="https://raw.githubusercontent.com/itiger-cluster/itiger-cluster.github.io/master/assets/img/nsf_official_logo.png" class="img-fluid pb-3">
		</div>
		</div>
  </div>


  {% if page.related_publications %}
    <h2>References</h2>
    <div class="publications">
      {% bibliography --cited_in_order %}
    </div>
  {% endif %}

  {% if site.giscus and page.giscus_comments %}
    {% include giscus.liquid %}
  {% endif %}
</div>
