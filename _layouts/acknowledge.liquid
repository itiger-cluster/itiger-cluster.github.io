---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title">{{ page.title }}</h1>
    <p class="post-description">{{ page.description }}</p>
  </header>

  <div class="container">
		<div class='row'>
		<div class="col-md-6">
		<article>
    {{ content }}
    </article>
		</div>
		<div class="col-md-6">
		<img src="https://nsf.widen.net/content/txvhzmsofh/png/NSF_Official_logo_High_Res_1200ppi?u=rpch1x&position=c&quality=80" class="img-fluid">
		</div>
		</div>
  </div>


  {% if page.related_publications %}
    <h2>References</h2>
    <div class="publications">
      {% bibliography --cited_in_order %}
    </div>
  {% endif %}

  {% if site.giscus and page.giscus_comments %}
    {% include giscus.liquid %}
  {% endif %}
</div>
