---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title">{{ page.title }}</h1>
    <p class="post-description">{{ page.description }}</p>
  </header>

  <div class="container">
   <center><img src="https://itiger-cluster.github.io/assets/img/head_node-480.webp" class="img-fluid"></center>
  <center><img src="https://itiger-cluster.github.io/assets/img/compute_node-480.webp" class="img-fluid"></center>
  </div>
  <article>
    {{ content }}
  </article>

  {% if page.related_publications %}
    <h2>References</h2>
    <div class="publications">
      {% bibliography --cited_in_order %}
    </div>
  {% endif %}

  {% if site.giscus and page.giscus_comments %}
    {% include giscus.liquid %}
  {% endif %}
</div>
